  "components": {
    "schemas": {
      "VeiculoDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "placa": {
            "type": "string",
            "pattern": "^[A-Z]{3}[0-9][0-9A-Z][0-9]{2}$",
            "example": "ABC1D23"
          },
          "marca": {
            "type": "string",
            "example": "Volvo"
          },
          "quilometragem": {
            "type": "integer",
            "minimum": 0,
            "example": 150000
          },
          "status": {
            "type": "string",
            "enum": [
              "ATIVO",
              "INATIVO",
              "MANUTENCAO"
            ],
            "example": "ATIVO"
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataAtualizacao": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "VeiculoCreateDTO": {
        "type": "object",
        "required": [
          "placa",
          "marca",
          "quilometragem"
        ],
        "properties": {
          "placa": {
            "type": "string",
            "pattern": "^[A-Z]{3}[0-9][0-9A-Z][0-9]{2}$",
            "example": "ABC1D23"
          },
          "marca": {
            "type": "string",
            "example": "Volvo"
          },
          "quilometragem": {
            "type": "integer",
            "minimum": 0,
            "example": 150000
          },
          "status": {
            "type": "string",
            "enum": [
              "ATIVO",
              "INATIVO",
              "MANUTENCAO"
            ],
            "example": "ATIVO"
          }
        }
      },
      "VeiculoComPneusDTO": {
        "allOf": [
          {
            "$ref": "#/components/schemas/VeiculoDTO"
          },
          {
            "type": "object",
            "properties": {
              "pneusAplicados": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/VeiculoPneuDTO"
                }
              }
            }
          }
        ]
      },
      "PneuDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "numeroFogo": {
            "type": "string",
            "example": "P001"
          },
          "marca": {
            "type": "string",
            "example": "Michelin"
          },
          "pressaoAtual": {
            "type": "number",
            "format": "double",
            "minimum": 0,
            "example": 32
          },
          "status": {
            "type": "string",
            "enum": [
              "DISPONIVEL",
              "APLICADO",
              "DESGASTADO"
            ],
            "example": "DISPONIVEL"
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataAtualizacao": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "PneuCreateDTO": {
        "type": "object",
        "required": [
          "numeroFogo",
          "marca",
          "pressaoAtual"
        ],
        "properties": {
          "numeroFogo": {
            "type": "string",
            "example": "P001"
          },
          "marca": {
            "type": "string",
            "example": "Michelin"
          },
          "pressaoAtual": {
            "type": "number",
            "format": "double",
            "minimum": 0,
            "example": 32
          },
          "status": {
            "type": "string",
            "enum": [
              "DISPONIVEL",
              "APLICADO",
              "DESGASTADO"
            ],
            "example": "DISPONIVEL"
          }
        }
      },
      "VeiculoPneuDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "veiculoId": {
            "type": "integer",
            "format": "int64"
          },
          "pneuId": {
            "type": "integer",
            "format": "int64"
          },
          "position": {
            "type": "string",
            "example": "DIANTEIRA_ESQUERDA"
          },
          "numeroFogo": {
            "type": "string",
            "example": "P001"
          },
          "marca": {
            "type": "string",
            "example": "Michelin"
          },
          "pressaoAtual": {
            "type": "number",
            "format": "double",
            "example": 32
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time"
          },
          "dataAtualizacao": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 400
          },
          "error": {
            "type": "string",
            "example": "Erro de validação"
          },
          "message": {
            "type": "string",
            "example": "Placa já cadastrada: ABC1D23"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "details": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "properties": {}
          }
        }
      }
    },
